<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:v="using:AvaloniaApp.Views.Protection"
             x:Class="AvaloniaApp.Views.Protection.ProtectionRemoveView">
  <v:ProtectionCommonView>
    <v:ProtectionCommonView.Child>
      <StackPanel Spacing="8">
        <Button Content="Unprotect" Command="{Binding UnprotectCommand}" />
      </StackPanel>
    </v:ProtectionCommonView.Child>
  </v:ProtectionCommonView>
</UserControl>


